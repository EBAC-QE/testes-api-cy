pipeline {

  agent any

  stages {

    stage('Clonar repositÃ³rio') {

      steps {

        git branch: 'main', url: 'https://github.com/RenanMitsuo/testes-api-cy.git'

      }

    }

    stage('Instalar dependencias') {

      steps {

        bat 'npm install'

      }

    }


    stage('Realizar os testes') {

      steps {

        bat 'npm run cy:run'

      }

    }

  }

}